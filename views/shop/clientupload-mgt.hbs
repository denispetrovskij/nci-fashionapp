<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h3>Upload Review Tool</h3>
        </div>
    </div>
</div>
<br>
<br>
<div class="container">
{{# each clients }}
    <div class="row">
    {{# each this }}
        <div class="col-md-4">
            <!-- begin panel group -->
            <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                <div class="panel panel-default">
                    <span class="side-tab" data-target="#tab2" data-toggle="tab" role="tab" aria-expanded="false">
                        <h6>Upload ID: {{this._id}}</h6>
                    </span>
                </div>
                <div class="panel panel-default">
                        <h6>User ID: {{this.user}}</h6>
                    </span>
                </div>
                <div class="panel panel-default">
                    <h6>Product ID: {{this.productid}}</h6>
                    </span>
                </div>
                <div class="panel panel-success">
                    <a href="/approve/{{this._id}}" class="panel-heading btn btn-success btn-lg btn-block"><span class="glyphicon glyphicon-ok"></span> APPROVE</a>
                </div>
                <div class="panel panel-danger">
                    <a href="/disapprove/{{this._id}}" class="panel-heading btn btn-danger btn-lg btn-block"><span class="glyphicon glyphicon-remove"></span> DISAPPROVE</a>
                </div>
            </div>
        </div>

        <div class="col-md-8">
            <!-- begin macbook pro mockup -->
            <div class="md-macbook-pro md-glare">
                <div class="md-lid">
                    <div class="md-camera"></div>
                    <div class="md-screen">
                        <div class="tab-featured-image">
                            <div class="tab-content">
                                <div class="tab-pane  in active" id="tab1">
                                    <img src="{{ this.clientImagePath}}" alt="tab1" class="img img-responsive">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                </div>
        </div>
    {{/each}}
    </div>
    {{/each}}
</div>
